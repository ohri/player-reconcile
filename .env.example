# Oracle Database Connection Settings
# Copy this file to .env and fill in your actual values

# Oracle database username
ORACLE_USER=your_username

# Oracle database password
ORACLE_PASSWORD=your_password

# Oracle database host (IP or hostname)
ORACLE_HOST=192.168.1.100

# Oracle database port (default: 1521)
ORACLE_PORT=1521

# Oracle service name (not SID)
ORACLE_SERVICE=your_service_name

# Example for remote VPN connection:
# ORACLE_HOST=10.0.0.50
# ORACLE_PORT=1521
# ORACLE_SERVICE=NETFL
# ORACLE_USER=netfl_user
# ORACLE_PASSWORD=secure_password

# Note: Never commit the actual .env file to version control!
# Add .env to your .gitignore
